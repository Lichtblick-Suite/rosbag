{
  "name": "rosbag",
  "version": "2.6.3",
  "license": "Apache-2.0",
  "repository": "cruise-automation/rosbag.js",
  "dependencies": {
    "buffer": "6.0.3",
    "heap": "0.2.6",
    "int53": "1.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/node",
  "browser": "dist/web",
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "yarn lint --fix",
    "test": "yarn run jest --verbose --testPathIgnorePatterns 'web'",
    "clean": "rm -rf build dist",
    "docs": "yarn typedoc",
    "build": "yarn run clean && mkdir -p dist && yarn run flow && yarn run build-node && yarn run build-web && yarn run flow-copy-source src dist",
    "build-node": "cross-env ROSBAG_TARGET=node webpack --mode development",
    "build-web": "cross-env ROSBAG_TARGET=web webpack --mode development"
  },
  "devDependencies": {
    "@types/heap": "0.2.28",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "compressjs": "1.0.3",
    "cross-env": "5.2.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "lz4js": "0.2.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.0",
    "typedoc": "0.20.23",
    "typescript": "4.1.3"
  },
  "prettier": {
    "arrowParens": "always",
    "jsxBracketSameLine": true,
    "printWidth": 120,
    "trailingComma": "es5"
  }
}
